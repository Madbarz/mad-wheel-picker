{
  "name": "mad-wheel-picker",
  "version": "0.1.4",
  "description": "Native android wheel picker that resembles iOS wheel picker",
  "main": "src/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "!lib/typescript/example",
    "!android/build",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it --no-npm.publish",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn && yarn example pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/Madbarz/mad-wheel-picker",
  "author": "madbarz <madbarz@madbarz.com> (https://github.com/Madbarz)",
  "bugs": {
    "url": "https://github.com/Madbarz/mad-wheel-picker/issues"
  },
  "homepage": "https://github.com/Madbarz/mad-wheel-picker#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.10",
    "@commitlint/config-conventional": "^17.7.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@release-it/conventional-changelog": "^7.0.0",
    "@types/react": "~18.2.20",
    "@types/react-native": "0.72.2",
    "commitlint": "^17.7.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "pod-install": "^0.1.38",
    "prettier": "^3.0.2",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-builder-bob": "^0.21.3",
    "release-it": "^16.1.5",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@react-native-picker/picker": "^2.5.0"
  },
  "resolutions": {
    "@types/react": "18.2.20"
  },
  "peerDependencies": {
    "@react-native-picker/picker": "^2.5.0",
    "react": "*",
    "react-native": "*"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
